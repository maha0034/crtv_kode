        <Card img="/imgs/graffiti02.webp" tag="Køb brugt" text="Dolor sit amet consectetur Lorem ipsum" />
        <Card img="/imgs/graffiti03.webp" tag="Nørrebros bygninger" text="Qui et debitis sit illum consequatur" />
